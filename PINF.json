{
    "@extends": {
        "ScriptedCloud": "/pinf.genesis/03-pinf.tools.ScriptedCloud/PINF.proto.json",
        "vm.pinf.io": "/pinf.genesis/03-vm.pinf.io/PINF.proto.json",
        "to.pinf.dnsimple": {
            "location": "/pinf.genesis/03-pinf.tools.ScriptedCloud/PINF.proto/to.pinf.dnsimple.proto.patch.json"
        },
        "io.pinf.db.postgresql": {
            "location": "/pinf.genesis/03-vm.pinf.io/PINF.proto/io.pinf.db.postgresql~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "roles": {},
                "databases": {}
            }
        },
        "io.pinf.server.grunt": {
            "location": "/pinf.genesis/03-vm.pinf.io/PINF.proto/io.pinf.server.grunt~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "grunts": {
                    "local": {},
                    "remote": {}
                }
            }
        },
        "io.pinf.server.webpack": {
            "location": "/pinf.genesis/03-vm.pinf.io/PINF.proto/io.pinf.server.webpack~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "packs": {
                    "local": {},
                    "remote": {}
                }
            }
        },
        "to.pinf.nwjs": {
            "location": "/pinf.genesis/04-devcomp.org/PINF.proto/github.com~pinf-to~pinf-to-nwjs~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{env.PGS_WORKSPACE_ROOT}}",
                "PGS_PACKAGES_DIRPATH": "{{env.PGS_PACKAGES_DIRPATH}}",
                "install": true
            }
        }
    },
    "config": {
        "ScriptedCloud.PINF.Tools/boot/0": {
            "$depends": [
                "devcomp.org-dev-online"
            ]
        },
        "devcomp.org/dev/0": {
            "$to": "devcomp.org-dev",
            "$depends": [
                "devcomp.org-dev-local",
                "devcomp.org-dev-online"
            ]
        },
        "devcomp.org/dev-local/0": {
            "$to": "devcomp.org-dev-local",
            "$depends": [
                "webpack-service",
                "grunt-service",
                "services-proxy"
            ]
        },
        "devcomp.org/dev-online/0": {
            "$to": "devcomp.org-dev-online",
            "$depends": [
                "dns",
                "pio-sync-services"
            ]
        },
        "ScriptedCloud.PINF.Tools/0": {
            "topLevelDomain": "pinf.me",
            "hostname": "{{env.PGS_WORKSPACE_UID}}.devcomp.github.pinf.me",
            "dirname": "devcomp.github.pinf.me~{{env.PGS_WORKSPACE_UID}}",
            "privateKeyPath": "{{env.HOME}}/.ssh/devcomp.github.pinf.me-{{env.PGS_WORKSPACE_UID}}.rsa"
        }
    },
    "mappings": {},
    "@overlays": [
        "{{env.PIO_PROFILE_PATH}}"
    ]
}