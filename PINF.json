{
    "@extends": {
        "devcomp.org": "{{__DIRNAME__}}/PINF.proto.json"
    },
    "config": {
        "ScriptedCloud.PINF.Tools/boot/0": {
            "$depends": [
                "devcomp.org-dev"
            ]
        },
        "ScriptedCloud.PINF.Tools/0": {
            "topLevelDomain": "pinf.me",
            "hostname": "{{env.PGS_WORKSPACE_UID}}.devcomp.github.pinf.me",
            "dirname": "devcomp.github.pinf.me~{{env.PGS_WORKSPACE_UID}}",
            "privateKeyPath": "{{env.HOME}}/.ssh/devcomp.github.pinf.me-{{env.PGS_WORKSPACE_UID}}.rsa"
        }
    },
    "mappings": {
        "{{env.PGS_PACKAGES_DIRPATH}}/github.com~devcomp-io~io.devcomp.tool.intern~0/source/installed/master": {
            "install": true
        },
        "bash.origin.nw": {
            "install": true
        },
        "{{env.PGS_PACKAGES_DIRPATH}}/github.com~devcomp~org.devcomp.portal.ComponentTree~0/source/installed/master": {
            "install": true
        },
        "org.devcomp.portal.ComponentTree": {
            "install": true
        },
        "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-logic~pinf.logic-for-devcomp.portal.ComponentTree~0/source/installed/master": {
            "install": true
        },
        "pinf.logic-for-devcomp.portal.ComponentTree": {
            "install": true
        },
        "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-it~pinf-it-bundler~0/source/installed/master": {
            "install": true
        },
        "pinf-it-bundler": {
            "install": true
        },
        "{{env.PGS_PACKAGES_DIRPATH}}/github.com~bash-origin~bash.origin.nw~0/source/installed/master": {
            "install": true
        },
        "bash.origin.nw": {
            "install": true
        },
        "{{env.PGS_PACKAGES_DIRPATH}}/github.com~pinf-to~pinf-to-nwjs~0/source/installed/master": {
            "install": true
        },
        "to.pinf.nwjs": {
            "install": true
        }
    },
    "@overlays": [
        "{{env.PIO_PROFILE_PATH}}"
    ]
}