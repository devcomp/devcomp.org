{
    "@extends": {
        "03-vm.pinf.io/PINF.core.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-vm.pinf.io/PINF.core.json",
            "@args": {}
        },
        "03-pinf.tools.ScriptedCloud/PINF.core.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-pinf.tools.ScriptedCloud/PINF.core.json",
            "@args": {}
        },
        "03-pinf.tools.ScriptedCloud/PINF.proto.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-pinf.tools.ScriptedCloud/PINF.proto.json",
            "@args": {}
        },
        "03-vm.pinf.io/PINF.proto.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-vm.pinf.io/PINF.proto.json",
            "@args": {}
        },
        "03-pinf.tools.ScriptedCloud/PINF.proto/to.pinf.dnsimple.proto.patch.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-pinf.tools.ScriptedCloud/PINF.proto/to.pinf.dnsimple.proto.patch.json",
            "@args": {}
        },
        "03-vm.pinf.io/PINF.proto/io.pinf.server.grunt~0.proto.patch.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-vm.pinf.io/PINF.proto/io.pinf.server.grunt~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{__DIRNAME__}}",
                "PGS_PACKAGES_DIRPATH": "{{__DIRNAME__}}/.deps",
                "grunts": {
                    "local": {},
                    "remote": {}
                }
            }
        },
        "03-vm.pinf.io/PINF.proto/io.pinf.server.webpack~0.proto.patch.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/03-vm.pinf.io/PINF.proto/io.pinf.server.webpack~0.proto.patch.json",
            "@args": {
                "PGS_WORKSPACE_ROOT": "{{__DIRNAME__}}",
                "PGS_PACKAGES_DIRPATH": "{{__DIRNAME__}}/.deps",
                "packs": {
                    "local": {},
                    "remote": {}
                }
            }
        },
        "04-devcomp.org/PINF.core.json": {
            "location": "{{env.PGS_PACKAGES_DIRPATH}}/04-devcomp.org/PINF.core.json",
            "@args": {}
        }
    },
    "config": {
        "github.com/pinf-to/pinf-to-process/0": {
            "$to": {
                "services-proxy": {
                    "basePath": "{{__DIRNAME__}}/.deps/github.com~pinf-io~io.pinf.proxy~0/source/installed/master",
                    "commands": {
                        "start": "node server.js"
                    },
                    "boot": {
                        "config": "io.pinf.proxy/0"
                    },
                    "config": {
                        "io.pinf.proxy/0": {
                            "port": 8081,
                            "bind": "127.0.0.1"
                        }
                    }
                },
                "grunt-service": {
                    "basePath": "{{__DIRNAME__}}/.deps/github.com~pinf-io~io.pinf.server.grunt~0/source/installed/master",
                    "commands": {
                        "start": "./bin/run-dev-server"
                    },
                    "boot": {
                        "config": "io.pinf.server.grunt/0"
                    },
                    "env": {
                        "VERBOSE": "1",
                        "DEBUG": "1"
                    },
                    "config": {
                        "io.pinf.server.grunt/0": {
                            "port": 8083,
                            "bind": "127.0.0.1",
                            "grunts": {}
                        }
                    }
                },
                "webpack-service": {
                    "basePath": "{{__DIRNAME__}}/.deps/github.com~pinf-io~io.pinf.server.webpack~0/source/installed/master",
                    "commands": {
                        "start": "./bin/run-dev-server"
                    },
                    "boot": {
                        "config": "io.pinf.server.webpack/0"
                    },
                    "config": {
                        "io.pinf.server.webpack/0": {
                            "port": 8084,
                            "bind": "127.0.0.1",
                            "packs": {}
                        }
                    }
                }
            }
        },
        "github.com/pinf-to/pinf-to-openshift/0": {
            "openshift": {
                "cartridge": "nodejs-0.10",
                "publicKeyPath": "{{$from.space['$space.pinf.genesis/origin/0'].public.keyPath}}",
                "publicKeyName": "{{$from.system.dirname}}",
                "privateKeyPath": "{{$from.space['$space.pinf.genesis/origin/0'].secret.keyPath}}",
                "app": "devtools.{{$from.system.dirname}}"
            },
            "$to": "server-devtools",
            "env": {
                "PIO_HOME": "$HOME/app-root/data/io.pinf",
                "BO_ROOT_SCRIPT_PATH": "$HOME/app-root/data/io.pinf/bin/bash.origin",
                "BO_SYSTEM_CACHE_DIR": "$HOME/app-root/data/io.pinf/cache/bash.origin.cache"
            }
        },
        "github.com/pinf-io/io.pinf.pio.sync/0": {
            "$to": {
                "pio-sync-services": {
                    "ssh": {
                        "ip": "{{$from.server-services.vm.ip}}",
                        "user": "{{$from.server-services.dropplet.sshUser}}",
                        "privateKeyPath": "{{$from.server-services.dropplet.keyPath}}"
                    },
                    "vfs-socket-server": {
                        "port": 8086,
                        "bind": "{{$from.server-services.vm.ip}}",
                        "privateKeyPath": "{{$from.server-services.dropplet.keyPath}}"
                    },
                    "env": "{{$from.server-services.env}}",
                    "adapters": {
                        "os": "pinf.io-for-ubuntu"
                    },
                    "services": {
                        "bash.origin": {
                            "location": "bash.origin"
                        },
                        "pinf.io-for-ubuntu": {
                            "depends": [
                                "bash.origin"
                            ],
                            "location": "pinf.io-for-ubuntu",
                            "config": {
                                "pio.pinf.io/0": {
                                    "on": {
                                        "postactivate": "bin/install"
                                    }
                                }
                            }
                        },
                        "pinf-it-package-insight": {
                            "depends": [
                                "pinf.io-for-ubuntu"
                            ],
                            "location": "pinf-it-package-insight"
                        },
                        "smi-for-npm": {
                            "depends": [
                                "pinf.io-for-ubuntu",
                                "bash.origin",
                                "pinf-it-package-insight"
                            ],
                            "location": "smi-for-npm",
                            "config": {
                                "pio.pinf.io/0": {
                                    "on": {
                                        "postactivate": [
                                            "bin/link-to $PIO_BIN_DIRPATH/smi-for-npm",
                                            "smi-for-npm relink"
                                        ]
                                    }
                                }
                            }
                        },
                        "org.pinf.lib": {
                            "depends": [
                                "smi-for-npm"
                            ],
                            "location": "org.pinf.lib"
                        },
                        "org.pinf.genesis.lib": {
                            "depends": [
                                "org.pinf.lib"
                            ],
                            "location": "org.pinf.genesis.lib"
                        },
                        "pinf.io-for-unix": {
                            "depends": [
                                "bash.origin",
                                "org.pinf.genesis.lib"
                            ],
                            "location": "pinf.io-for-unix",
                            "config": {
                                "org.pinf.genesis.lib/0": {
                                    "api": {
                                        "consumes": {
                                            "FLAVOR": "pinf.io-for-ubuntu/0/manifested-os"
                                        }
                                    }
                                }
                            }
                        },
                        "io.pinf.pio.postsync": {
                            "depends": [
                                "bash.origin",
                                "pinf.io-for-unix",
                                "org.pinf.genesis.lib"
                            ],
                            "location": "io.pinf.pio.postsync",
                            "config": {
                                "org.pinf.genesis.lib/0": {
                                    "api": {
                                        "consumes": {
                                            "OS": "pinf.io-for-unix/0/virtual-os"
                                        }
                                    }
                                }
                            }
                        },
                        "vfs-local": {
                            "depends": [
                                "io.pinf.pio.postsync"
                            ],
                            "location": "vfs-local"
                        },
                        "vfs-socket": {
                            "depends": [
                                "vfs-local"
                            ],
                            "location": "vfs-socket"
                        },
                        "io.pinf.pio.sync": {
                            "depends": [
                                "io.pinf.pio.postsync",
                                "vfs-local",
                                "vfs-socket"
                            ],
                            "location": "io.pinf.pio.sync",
                            "config": {
                                "io.pinf.pio.sync/0": {
                                    "vfs-socket-server": {
                                        "port": 8086,
                                        "bind": "{{$from.server-services.vm.ip}}",
                                        "publicKey": "{{$from.space['$space.pinf.genesis/origin/0'].public.pem}}"
                                    }
                                }
                            }
                        },
                        "io.pinf.proxy": {
                            "depends": [
                                "io.pinf.pio.postsync",
                                "io.pinf.pio.sync"
                            ],
                            "location": "io.pinf.proxy",
                            "boot": {
                                "config": "io.pinf.proxy/0"
                            },
                            "config": {
                                "io.pinf.proxy/0": {
                                    "port": 8085,
                                    "bind": "{{$from.server-services.vm.ip}}",
                                    "services": {
                                        "io.pinf.pio.sync.vfs.rest": {
                                            "implementation": "SYNC_VFS_REST"
                                        }
                                    },
                                    "vhosts": {
                                        "io-pinf-pio-sync-vfs-socket.{{$from.system.hostname}}": "{{$from.server-services.vm.ip}}:8086"
                                    }
                                },
                                "org.pinf.genesis.lib/0": {
                                    "api": {
                                        "consumes": {
                                            "SYNC_VFS_REST": "io.pinf.pio.sync/0/vfs-rest"
                                        }
                                    }
                                }
                            }
                        },
                        "io.pinf.server.grunt": {
                            "depends": [
                                "bash.origin",
                                "io.pinf.pio.postsync",
                                "org.pinf.genesis.lib",
                                "org.pinf.lib",
                                "io.pinf.proxy"
                            ],
                            "location": "{{__DIRNAME__}}/node_modules/io.pinf.server.grunt",
                            "boot": {
                                "config": "io.pinf.server.grunt/0"
                            },
                            "config": {
                                "io.pinf.server.grunt/0": {
                                    "port": 8083,
                                    "bind": "{{$from.server-services.vm.ip}}",
                                    "grunts": {}
                                }
                            }
                        },
                        "io.pinf.server.webpack": {
                            "depends": [
                                "bash.origin",
                                "io.pinf.pio.postsync",
                                "org.pinf.genesis.lib",
                                "org.pinf.lib",
                                "io.pinf.proxy"
                            ],
                            "location": "{{__DIRNAME__}}/node_modules/io.pinf.server.webpack",
                            "boot": {
                                "config": "io.pinf.server.webpack/0"
                            },
                            "config": {
                                "io.pinf.server.webpack/0": {
                                    "port": 8084,
                                    "bind": "{{$from.server-services.vm.ip}}",
                                    "packs": {}
                                }
                            }
                        }
                    }
                }
            }
        },
        "ScriptedCloud.PINF.Tools/0": {
            "$to": "system",
            "$depends": [
                "smi-git-catalog",
                "smi-npm-catalog",
                "ScriptedCloud.PINF.Tools-commands"
            ]
        },
        "ScriptedCloud.PINF.Tools/ci/0": {
            "$to": "ScriptedCloud.PINF.Tools-ci",
            "$depends": [
                "server-testing"
            ]
        },
        "ScriptedCloud.PINF.Tools/expand-only/0": {
            "$to": "ScriptedCloud.PINF.Tools-expand-only"
        },
        "ScriptedCloud.PINF.Tools/boot/0": {
            "$to": "ScriptedCloud.PINF.Tools",
            "$depends": [
                "dns"
            ]
        },
        "github.com/pinf-tools/CloudCommands/0": {
            "$to": "ScriptedCloud.PINF.Tools-commands",
            "tools.pinf.CloudCommands/commands/0": {
                "register": true
            }
        },
        "github.com/sourcemint/smi-for-git/0": {
            "$to": "smi-git-catalog",
            "export": {
                "catalog": "{{__DIRNAME__}}/smi.json"
            }
        },
        "github.com/sourcemint/smi-for-npm/0": {
            "$to": "smi-npm-catalog",
            "declaredMappings": "{{$from.smi-git-catalog.declaredMappings}}",
            "export": {
                "catalog": "{{__DIRNAME__}}/smi.json"
            }
        },
        "github.com/pinf-space/Genesis/0": {
            "$to": "space",
            "space.pinf.genesis/origin/0": {
                "@impl": "io.pinf.pio.profile/space.pinf.genesis-origin/0",
                "id": "{{env.PIO_PROFILE_KEY}}",
                "label": "{{$from.system.dirname}}",
                "secret": {
                    "keyPath": "{{$from.system.privateKeyPath}}"
                },
                "stores": {
                    "primary": {
                        "@impl": "com.amazon.aws/s3/0",
                        "s3": {
                            "region": "us-west-1",
                            "publicHost": "s3-us-west-1.amazonaws.com",
                            "path": "{{$from.system.dirname}}"
                        }
                    }
                }
            },
            "space.pinf.genesis/access/0": {
                "@impl": "io.pinf.pio.profile/space.pinf.genesis-access/0"
            },
            "tools.pinf.CloudCommands/commands/0": {
                "commands": {
                    "root.space.invite": {
                        "@impl": "space.pinf.genesis/origin.get-invite-token/0",
                        "label": "[ROOT] Invite someone into this space on EQUAL TERMS AS YOU!",
                        "options": {
                            "scope": "root"
                        }
                    }
                }
            }
        },
        "github.com/pinf-io/io.pinf.pio.profile/0": {
            "$to": "profile",
            "space.pinf.genesis/origin/0": "{{$from.space['$space.pinf.genesis/origin/0']}}",
            "space.pinf.genesis/access/0": "{{$from.space['$space.pinf.genesis/access/0']}}",
            "files": {
                "profile.json": {
                    "path": "{{__DIRNAME__}}.profile.json"
                },
                "activate.sh": {
                    "path": "{{activatePathFromProfilePath({{__DIRNAME__}}.profile.json)}}"
                },
                "ssh.key": {
                    "path": "{{$from.system.privateKeyPath}}"
                }
            }
        },
        "github.com/pinf-to/pinf-to-digitalocean/0": {
            "$to": "server-services",
            "credentials": "{{$from.profile.credentials['digitalocean.com']}}",
            "dropplet": {
                "name": "{{$from.system.dirname}}",
                "keyId": "{{$from.system.dirname}}",
                "keyPath": "{{$from.space['$space.pinf.genesis/origin/0'].secret.keyPath}}",
                "keyPubPath": "{{$from.space['$space.pinf.genesis/origin/0'].public.keyPath}}"
            },
            "env": {
                "PIO_HOME": "$HOME/io.pinf",
                "BO_ROOT_SCRIPT_PATH": "$HOME/io.pinf/bin/bash.origin",
                "BO_SYSTEM_CACHE_DIR": "$HOME/io.pinf/cache/bash.origin.cache"
            }
        },
        "github.com/pinf-to/pinf-to-travis-ci/0": {
            "$to": "server-testing"
        },
        "devcomp.org/dev/0": {
            "$to": "devcomp.org-dev",
            "$depends": [
                "devcomp.org-dev-local",
                "devcomp.org-dev-online"
            ]
        },
        "devcomp.org/dev-local/0": {
            "$to": "devcomp.org-dev-local",
            "$depends": [
                "webpack-service",
                "grunt-service",
                "services-proxy"
            ]
        },
        "devcomp.org/dev-online/0": {
            "$to": "devcomp.org-dev-online",
            "$depends": [
                "dns",
                "pio-sync-services"
            ]
        },
        "github.com/pinf-to/pinf-to-dnsimple/0": {
            "$to": "dns",
            "credentials": "{{$from.profile.credentials['dnsimple.com']}}",
            "github.com/pinf-logic/pinf.logic-for-dns/0": {
                "records": {
                    "{{$from.system.hostname}}": {
                        "domain": "{{$from.system.topLevelDomain}}",
                        "type": "A",
                        "data": "{{$from.server-services.vm.ip}}"
                    },
                    "*.{{$from.system.hostname}}": {
                        "domain": "{{$from.system.topLevelDomain}}",
                        "type": "CNAME",
                        "data": "{{$from.system.hostname}}"
                    }
                }
            }
        }
    },
